{
  "name": "what-s-that-chord-doing",
  "version": "0.0.1",
  "description": "An audio-based, \"tonally-functional\" chord identification game",
  "main": "js/index.js",
  "engines": {
    "node": "7.2.1"
  },
  "scripts": {
    "start": "node server.js",
    "postinstall": "run-s build",
    "dev": "run-p watch serve",
    "test": "run-s test-frontend test-backend",
    "test-frontend": "jest",
    "test-frontend-interactve": "jest --watch",
    "test-backend": "mocha --recursive --compilers js:@babel/register test-server",
    "build": "run-s build:*",
    "clean": "rimraf build",
    "serve": "npm start",
    "watch": "run-p watch:*",
    "mkdir": "mkdirp build",
    "clean:html": "rimraf build/index.html",
    "mkdir:html": "run-s mkdir",
    "build:html": "run-s clean:html mkdir:html _build:html",
    "_build:html": "cpr index.html build/index.html",
    "watch:html": "run-s build:html _watch:html",
    "_watch:html": "chokidar index.html -c \"npm run build:html\"",
    "clean:js": "rimraf build/js",
    "mkdir:js": "mkdirp build/js",
    "build:js": "run-s clean:js mkdir:js _build:js",
    "_build:js": "webpack",
    "watch:js": "run-s build:js _watch:js",
    "_watch:js": "webpack --watch",
    "clean:css": "rimraf build/css",
    "mkdir:css": "run-s mkdir",
    "build:css": "run-s clean:css mkdir:css _build:css",
    "_build:css": "cpr css build/css",
    "watch:css": "run-s build:css _watch:css",
    "_watch:css": "chokidar css/**/* -c \"npm run build:css\"",
    "mkdir:assets": "run-s mkdir",
    "clean:assets": "rimraf build/assets",
    "build:assets": "run-s clean:assets mkdir:assets _build:assets",
    "_build:assets": "cpr assets build/assets",
    "watch:assets": "run-s build:assets _watch:assets",
    "_watch:assets": "chokidar assets/**/* -c \"npm run build:assets\""
  },
  "author": {
    "name": "Dan Rounds",
    "email": "dan rounds at g m a i l dot com"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.1",
    "chai-http": "^4.2.0",
    "chokidar-cli": "^1.2.2",
    "cpr": "^2.0.0",
    "enzyme": "^2.4.1",
    "enzyme-to-json": "^2.0.0",
    "http-server": "^0.11.1",
    "jest": "^24.5.0",
    "jest-localstorage-mock": "^2.0.1",
    "jsdom": "^11.2.0",
    "mkdirp": "^0.5.1",
    "mocha": "^5.2.0",
    "npm-run-all": "^3.0.0",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "badwords": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "braces": "^2.3.2",
    "cors": "^2.8.4",
    "express": "^4.15.3",
    "isomorphic-fetch": "^2.2.1",
    "jwt-simple": "^0.5.1",
    "mongoose": "^4.11.4",
    "morgan": "^1.8.2",
    "passport": "^0.3.2",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-responsive": "^1.3.0",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "soundfont-player": "^0.10.7",
    "vexflow": "^1.2.87"
  },
  "jest": {
    "setupFiles": [
      "jest-localstorage-mock"
    ]
  }
}
